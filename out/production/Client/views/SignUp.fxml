<AnchorPane prefHeight="400.0" prefWidth="257.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.SignUpController">
   <children>
      <AnchorPane fx:id="anchorPane" prefHeight="622.0" prefWidth="337.0" style="-fx-background-color: #24313c;">
         <children>
            <Button fx:id="btnSignUp" layoutX="140.0" layoutY="545.0" mnemonicParsing="false" onAction="#btnSignUpAction" prefHeight="36.0" prefWidth="97.0" style="-fx-background-color: #1ab188; -fx-cursor: hand;" text="Sign Up" textFill="WHITE" />
            <JFXTextField fx:id="txtUsername" layoutX="83.0" layoutY="118.0" prefHeight="25.0" prefWidth="207.0" promptText="Username" stylesheets="@../styles/TextFieldStyle.css" unFocusColor="#bcb1b1" />
            <JFXTextField fx:id="txtNickname" layoutX="83.0" layoutY="191.0" prefHeight="25.0" prefWidth="207.0" promptText="Nickname" stylesheets="@../styles/TextFieldStyle.css" unFocusColor="#bcb1b1" />
            <JFXTextField fx:id="txtPassword" accessibleRole="PASSWORD_FIELD" layoutX="83.0" layoutY="264.0" prefHeight="25.0" prefWidth="207.0" promptText="Password" stylesheets="@../styles/TextFieldStyle.css" unFocusColor="#bcb1b1" />
            <ImageView fitHeight="74.0" fitWidth="151.0" layoutX="94.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/tagami.png" />
               </image>
            </ImageView>
            <Label layoutX="92.0" layoutY="590.0" text="Have an account? " textFill="WHITE" />
            <Label fx:id="lblLogin" layoutX="211.0" layoutY="590.0" onMouseClicked="#lblLogInAction" style="-fx-cursor: hand;" text="Log In" textFill="#1ab188" underline="true" />
            <Label fx:id="lblNoti" layoutX="92.0" layoutY="511.0" textFill="#ff2108" />
            <ImageView fx:id="imgClose" fitHeight="25.0" fitWidth="34.0" layoutX="303.0" layoutY="6.0" onMouseClicked="#closeApp" pickOnBounds="true" preserveRatio="true" style="-fx-cursor: hand;">
               <image>
                  <Image url="@../images/close.png" />
               </image>
            </ImageView>
            <ImageView fitHeight="36.0" fitWidth="34.0" layoutX="32.0" layoutY="113.0" pickOnBounds="true" preserveRatio="true" />
            <ImageView fitHeight="36.0" fitWidth="34.0" layoutX="40.0" layoutY="116.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/user.png" />
               </image>
            </ImageView>
            <ImageView fitHeight="30.0" fitWidth="34.0" layoutX="40.0" layoutY="184.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/nickname.png" />
               </image>
            </ImageView>
            <ImageView fitHeight="30.0" fitWidth="30.0" layoutX="42.0" layoutY="262.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/password.png" />
               </image>
            </ImageView>
            <Label layoutX="150.0" layoutY="342.0" text="Server Detail" textFill="WHITE">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Label layoutX="103.0" layoutY="354.0" text="------------------------------------" textFill="WHITE" />
            <JFXTextField fx:id="txtHostname" layoutX="133.0" layoutY="401.0" stylesheets="@../styles/TextFieldStyle.css" text="localhost" />
            <JFXTextField fx:id="txtPort" layoutX="133.0" layoutY="447.0" stylesheets="@../styles/TextFieldStyle.css" text="9999" />
            <Label layoutX="63.0" layoutY="412.0" text="Host name" textFill="WHITE" />
            <Label layoutX="81.0" layoutY="451.0" text="Port" textFill="WHITE" />
